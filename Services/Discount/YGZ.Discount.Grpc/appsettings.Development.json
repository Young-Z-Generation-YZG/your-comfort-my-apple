{
    "FeatureManagement": {
        "EnableSelfSsl": false
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DiscountDb": "Server=localhost;Port=4432;Database=DiscountDb;Username=bale;Password=bale;Include Error Detail=true",
        "RedisDb": "localhost:6379"
    },
    "GrpcSettings": {
        "CatalogUrl": "https://localhost:5051"
    },
    "OpenTelemetrySettings": {
        "OtelExporterOtlpEndpointSeq": "http://localhost:5341/ingest/otlp/v1/traces",
        "OtelExporterOtlpEndpointJaeger": "http://localhost:4317"
    },
    "MessageBrokerSettings": {
        "Host": "amqp://localhost:5672",
        "Username": "bale",
        "Password": "bale"
    },
    "Serilog": {
        "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Seq"],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information"
            }
        },
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "Seq",
                "Args": { "serverUrl": "http://localhost:5341/" }
            }
        ],
        "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
    }
}
